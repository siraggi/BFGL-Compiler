class Key begin
    dcl bool isPressed
end

class Sprite begin
    dcl num x
    dcl num y
    dcl num sizeX
    dcl num sizeY

    OnConstruct(num xx, num yy, num sizeXX, num sizeYY) do
        set x to xx
        set y to yy
        set sizeX to sizeXX
        set sizeY to sizeYY
    end

    dcl func update() begin

    end
end

